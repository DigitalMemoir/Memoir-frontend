{
  "manifest_version": 3,
  "name": "Memoir",
  "version": "1.0.2",
  "description": "Memoir is a Chrome extension that helps you manage your daily tasks and notes efficiently.",
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "scripting",
    "clipboardWrite",
    "history"
  ],
  "host_permissions": ["__API_URL__/*"],
  "web_accessible_resources": [
    {
      "resources": ["newtab/index.html", "popup/index.html"],
      "matches": ["chrome-extension://*/*"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' data: https://www.google.com https://t0.gstatic.com https://t1.gstatic.com https://t2.gstatic.com https://t3.gstatic.com https://picsum.photos https://lh3.googleusercontent.com;"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "key": ""
}
